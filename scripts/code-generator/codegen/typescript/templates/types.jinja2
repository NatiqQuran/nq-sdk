export interface {{ action.params_type.name }} {
{%- for prop_name, prop_type in action.params_type.properties.items() %}
    {%- if prop_name in action.params_type.required_properties %}
    {{ prop_name }}: {{ prop_type }};
    {%- else %}
    {{ prop_name }}?: {{ prop_type }};
    {%- endif %}
{%- endfor %}
}
{%- endif %}
{%- endfor %}
{%- endif %}
